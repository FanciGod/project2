<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox accessibleRole="BUTTON" prefHeight="750.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.project.Adminsite">
   <children>
      <MenuBar fx:id="menuBar" prefHeight="95.0" prefWidth="1024.0">
        <menus>
          <Menu fx:id="menu" mnemonicParsing="false" text="DashBoard" />
          <Menu mnemonicParsing="false" text="Products">
            <items>
              <MenuItem mnemonicParsing="false" text="Category" />
              <MenuItem mnemonicParsing="false" text="New product" />
                  <MenuItem mnemonicParsing="false" text="Inventory" />
                  <MenuItem mnemonicParsing="false" text="Discard product" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Transaction">
            <items>
              <MenuItem mnemonicParsing="false" text="Product sales" />
                  <MenuItem mnemonicParsing="false" text="Bill" />
                  <MenuItem mnemonicParsing="false" text="Import goods" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Partner">
              <items>
                  <MenuItem mnemonicParsing="false" text="Supplier" />
                <MenuItem mnemonicParsing="false" text="Customer" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Report">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
              </items>
            </Menu>
            <Menu fx:id="accountMenu" mnemonicParsing="false" text="Account">
              <items>
                <MenuItem mnemonicParsing="false" text="Inforamtion" />
                  <MenuItem mnemonicParsing="false" onAction="#regissite" text="Create new Account" />
                  <MenuItem mnemonicParsing="false" onAction="#fireBut" text="Log out" />
              </items>
            </Menu>
        </menus>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </MenuBar>
      <AnchorPane accessibleRole="MENU_ITEM" prefHeight="430.0" prefWidth="1024.0">
         <children>
            <Button fx:id="outBut" accessibleRole="MENU_ITEM" layoutX="1010.0" layoutY="-13.0" mnemonicParsing="false" onAction="#logout" prefHeight="0.0" prefWidth="1.0" visible="false" />
         </children>
      </AnchorPane>
   </children>
</VBox>
